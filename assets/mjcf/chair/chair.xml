		<motor name="joint1" joint="joint1" ctrllimited="true" ctrlrange="-1.57 1.57" forcelimited="true" forcerange="-.3 .3" gear="1"/>
  	<motor name="joint2" joint="joint2" ctrllimited="true" ctrlrange="-1.57 1.57" forcelimited="true" forcerange="-.3 .3" gear="1"/>
  	<motor name="joint3" joint="joint3" ctrllimited="true" ctrlrange="-1.57 1.57" forcelimited="true" forcerange="-.3 .3" gear="1"/>
  	<motor name="joint4" joint="joint4" ctrllimited="true" ctrlrange="-1.57 1.57" forcelimited="true" forcerange="-.3 .3" gear="1"/>
  	<motor name="joint5" joint="joint5" ctrllimited="true" ctrlrange="-1.57 1.57" forcelimited="true" forcerange="-.3 .3" gear="1"/>
  	<motor name="joint6" joint="joint6" ctrllimited="true" ctrlrange="-1.57 1.57" forcelimited="true" forcerange="-.3 .3" gear="1"/>
		-->
