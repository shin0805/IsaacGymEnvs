<mujoco model="chair">
  <default>
    <joint armature="0.001" limited="true"/>
    <geom condim="3" density="5.0" friction="1.5 0.1 0.1" margin="0.01" rgba="0.97 0.38 0.06 1"/>
  </default>

  <compiler inertiafromgeom="true" angle="radian"/>

  <option timestep="0.016" iterations="50" tolerance="1e-10" solver="Newton" jacobian="dense" cone="pyramidal"/>
	<option noslip_iterations="3"/>

  <size nconmax="50" njmax="200" nstack="10000"/>
  <visual>
      <map force="0.1" zfar="30"/>
      <rgba haze="0.15 0.25 0.35 1"/>
      <quality shadowsize="2048"/>
      <global offwidth="800" offheight="800"/>
  </visual>

  <asset>
		<texture name="texplane" type="2d" builtin="checker" rgb1=".8 .8 .8" rgb2=".6 .6 .6" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
		<material name="matplane" reflectance="0.3" texture="texplane" texrepeat="1 1" texuniform="true"/>
		<mesh name="face" scale="1e-3 1e-3 1e-3" file="./mesh/main-face-1.STL"/>
		<mesh name="leg1" scale="1e-3 1e-3 1e-3" file="./mesh/main-leg-4.STL"/>
		<mesh name="bracket1" scale="1e-3 1e-3 1e-3" file="./mesh/main-axis-3.STL"/>
		<mesh name="leg2" scale="1e-3 1e-3 1e-3" file="./mesh/main-leg-2.STL"/>
		<mesh name="bracket2" scale="1e-3 1e-3 1e-3" file="./mesh/main-axis-1.STL"/>
		<mesh name="leg3" scale="1e-3 1e-3 1e-3" file="./mesh/main-leg-3.STL"/>
		<mesh name="bracket3" scale="1e-3 1e-3 1e-3" file="./mesh/main-axis_mirror-1.STL"/>
		<mesh name="seat" scale="1e-3 1e-3 1e-3" file="./mesh/main-seat-1.STL"/>
  </asset>

  <worldbody>
		<geom name="floor" pos="0 0 0" size="0 0 .25" type="plane" material="matplane"/> <!-- condim="1" friction="1 0.005, 0.0001"/>-->
		<!--
    <geom name="floor" pos="0 0 0" size="0 0 .25" type="plane" material="matplane" condim="3"/>
		-->

    <light directional="false" diffuse=".2 .2 .2" specular="0 0 0" pos="0 0 5" dir="0 0 -1" castshadow="false"/>
    <light mode="targetbodycom" target="chair" directional="false" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 0 4.0" dir="0 0 -1"/>

		<body name="dummy" pos="0.095 0.0785 0.10365">
			<freejoint name="root"/>
			<!--<joint type="free"/>-->
		  <body name="chair" pos="-0.095 -0.0785 -0.10365">
		  	<geom name="seat" type="mesh" mesh="seat" rgba="1 .8 0 1"/>
		    <geom name="face" type="mesh" mesh="face" rgba="1 .8 0 1"/>

		  	<body>
		  			<!--<joint name="joint2" type="hinge" axis="0 1 0" damping=".010" pos=".12223 -.01378 .0061"/>-->
		  		<geom name="bracket1" type="mesh" mesh="bracket1" rgba="1 .8 0 1"/>
		  		<joint name="joint2" type="hinge" axis="0 1 0" damping=".010" pos=".13494 .06759 .10365" limited="true" range="-1.57 1.57"/>

		  	  <body>
		  	  		<!--<joint name="joint1" type="hinge" axis="1 0 0" damping=".010" pos=".12212 -.04497 .0061"/>-->
		  				<geom name="leg1" type="mesh" mesh="leg1" rgba=".5 .3 0 1"/> <!--condim="1" friction="0.1 0.0005, 0.00001"/>-->
		  	  	<joint name="joint1" type="hinge" axis="-1 0 0" damping=".010" pos=".13494 .0369 .10365" limited="true" range="-1.57 1.57"/>
		  	  </body>
		  	</body>


		  	<body>
		  			<!--<joint name="joint4" type="hinge" axis="0 1 0" damping=".010" pos=".06521 -.04447 .0061"/>-->
		  		<geom name="bracket2" type="mesh" mesh="bracket2" rgba="1 .8 0 1"/>
		  		<joint name="joint4" type="hinge" axis="1 0 0" damping=".010" pos=".07903 .0369 .10365" limited="true" range="-1.57 1.57"/>

		  	  <body>
		  	  		<!--<joint name="joint3" type="hinge" axis="1 0 0" damping=".010" pos=".03352 -.04436 .0061"/>-->
		  				<geom name="leg2" type="mesh" mesh="leg2" rgba=".5 .3 0 1"/> <!-- condim="1" friction="0.1 0.0005, 0.00001"/> -->
		  	  	<joint name="joint3" type="hinge" axis="0 1 0" damping=".010" pos=".04751 .0369 .10365" limited="true" range="-1.57 1.57"/>
		  	  </body>
		  	</body>


		  	<body>
		  			<!--<joint name="joint6" type="hinge" axis="0 1 0" damping=".010" pos=".06598 .04332 .10365"/>-->
		  		<geom name="bracket3" type="mesh" mesh="bracket3" rgba="1 .8 0 1"/>
		  		<joint name="joint6" type="hinge" axis="1 0 0" damping=".010" pos=".07903 .12513 .10365" limited="true" range="-1.57 1.57"/>

		  	  <body>
		  	  		<!--<joint name="joint5" type="hinge" axis="1 0 0" damping=".010" pos=".03479 .04376 .10365"/>-->
		  				<geom name="leg3" type="mesh" mesh="leg3" rgba=".5 .3 0 1"/> <!-- condim="1" friction="0.1 0.0005, 0.00001"/> -->
		  	  	<joint name="joint5" type="hinge" axis="0 -1 0" damping=".010" pos=".04751 .12513 .10365" limited="true" range="-1.57 1.57"/>
		  	  </body>
		  	</body>

		  </body>
		</body>
  </worldbody>

  <actuator>
		<position name="joint1" joint="joint1" kp="5" ctrllimited="true" ctrlrange="-1. 1." forcelimited="true" forcerange="-.04 .04"/>
  	<position name="joint2" joint="joint2" kp="5" ctrllimited="true" ctrlrange="-1. 1." forcelimited="true" forcerange="-.04 .04"/>
  	<position name="joint3" joint="joint3" kp="5" ctrllimited="true" ctrlrange="-1. 1." forcelimited="true" forcerange="-.04 .04"/>
  	<position name="joint4" joint="joint4" kp="5" ctrllimited="true" ctrlrange="-1. 1." forcelimited="true" forcerange="-.04 .04"/>
  	<position name="joint5" joint="joint5" kp="5" ctrllimited="true" ctrlrange="-1. 1." forcelimited="true" forcerange="-.04 .04"/>
  	<position name="joint6" joint="joint6" kp="5" ctrllimited="true" ctrlrange="-1. 1." forcelimited="true" forcerange="-.04 .04"/>
